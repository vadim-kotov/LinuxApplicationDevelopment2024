#: ../src/guess.c:39
#, c-format
msgid "Usage: %s [OPTION]\n"
msgstr "Использование: %s [ПАРАМЕТР]\n"

#: ../src/guess.c:40
#, c-format
msgid "Interactive number guessing game.\n"
msgstr "Интерактивная игра, угадывающая числа\n"

#: ../src/guess.c:41
#, c-format
msgid ""
"Choose number between 1 and 100 and answer questions with only 'yes' or "
"'no'\n"
"\n"
msgstr ""
"Загадайте число между 1 и 100 и отвечайте на вопросы только 'да' или 'нет'\n"
"\n"

#: ../src/guess.c:42
#, c-format
msgid "Options:\n"
msgstr "Параметры:\n"

#: ../src/guess.c:43
#, c-format
msgid "  -r             switch to Roman numerals\n"
msgstr "  -r             переключить на Римские цифры\n"

#: ../src/guess.c:44
#, c-format
msgid "  -h, --help     show this help\n"
msgstr "  -h, --help     показать эту справку\n"

#: ../src/guess.c:45
#, c-format
msgid "      --version  show version\n"
msgstr "      --version  показать версию программы\n"

#: ../src/guess.c:67
msgid "Invalid"
msgstr "Недопустимое число"

#: ../src/guess.c:143
#, c-format
msgid "Think of a number between 1 and 100\n"
msgstr "Загадайте число от 1 до 100\n"

#: ../src/guess.c:149
#, c-format
msgid "Is your number bigger than "
msgstr "Ваше число больше "

#: ../src/guess.c:151
#, c-format
msgid "? (y/n): "
msgstr "? (д/н): "

#: ../src/guess.c:159
#, c-format
msgid "Input read error\n"
msgstr "Ошибка чтения ввода\n"

#: ../src/guess.c:165
#, c-format
msgid "Input stopped by user\n"
msgstr "Ввод остановлен пользователем\n"

#: ../src/guess.c:171
msgid "y"
msgstr "д"

#: ../src/guess.c:175
msgid "n"
msgstr "н"

#: ../src/guess.c:181
#, c-format
msgid "Please, type 'y'(yes) or 'n'(no).\n"
msgstr "Пожалуйста, введите 'д'(да) или 'н'(нет)\n"

#: ../src/guess.c:189
#, c-format
msgid "Your number is: "
msgstr "Ваше число: "
